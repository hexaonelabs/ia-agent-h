{
  "name": "ia-agent-h",
  "version": "0.0.2",
  "description": "Autonomous agent for bolckchain financial services",
  "author": "hexaonelabs",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "build:ui:pwa": "rm -rf dist && ng build agent-h",
    "predeploy:ui:fb": "npm run build:ui:pwa",
    "deploy:ui:fb": "firebase deploy --only hosting:ai-agent-h",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:ui:pwa": "rm -rf dist && ng serve agent-h",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:pm2": "NODE_ENV=production pm2 start ./dist/main.js --name=agent-h",
    "stop:pm2": "pm2 stop agent-h",
    "delete:pm2": "pm2 delete agent-h",
    "restart:pm2": "pm2 restart agent-h",
    "monit:pm2": "pm2 monit",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "serve:ssr:agent-h": "node dist/platforms/browser/server/server.mjs"
  },
  "dependencies": {
    "@aave/contract-helpers": "^1.30.5",
    "@aave/math-utils": "^1.30.5",
    "@angular/animations": "^18.1.0",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-server": "^18.1.0",
    "@angular/router": "^18.1.0",
    "@angular/ssr": "^18.2.12",
    "@bgd-labs/aave-address-book": "^4.7.3",
    "@ionic/angular": "^8.4.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.15",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^8.1.1",
    "@walletconnect/ethereum-provider": "^2.17.4",
    "aos": "^2.3.4",
    "axios": "^1.7.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.13",
    "express": "^4.18.2",
    "hbs": "^4.2.0",
    "ionicons": "^7.0.0",
    "js-yaml": "^4.1.0",
    "marked": "^15.0.6",
    "openai": "^4.77.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.0",
    "twitter-api-v2": "^1.19.0",
    "viem": "^2.22.4",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.0",
    "@angular/cli": "^18.1.0",
    "@angular/compiler-cli": "^18.1.0",
    "@ionic/angular-toolkit": "^10.0.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/aos": "^3.0.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.18.0",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.49",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.5.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
